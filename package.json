    "js-yaml": "^4.1.0",
    "jwt-encode": "^1.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.22",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/dockerode": "^3.3.19",
    "@types/express": "^4.17.17",
    "@types/js-yaml": "^4.0.5",
    "@types/jwt-encode": "^1.0.0",
